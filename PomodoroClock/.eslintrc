{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier"
  ],
  "plugins": ["prettier", "react"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": { "ecmaVersion": 6, "sourceType": "module" },
  "rules": {
    "no-console": [
      "warn",
      {
        "allow": ["warn", "error"]
      }
    ],
    "no-unused-vars": ["warn"],
    "import/prefer-default-export": ["warn"],
    "import/no-extraneous-dependencies": ["warn"],
    "react/no-unused-state": ["warn"],
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true
      }
    ]
  }
}
